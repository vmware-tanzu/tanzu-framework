            - name: FELIX_LOGSEVERITYSCREEN
              value: info
            - name: FELIX_HEALTHENABLED
              value: "true"
            securityContext:
              privileged: true
            resources:
              requests:
                cpu: 250m
            livenessProbe:
              exec:
                command:
                - /bin/calico-node
                - -felix-live
                - -bird-live
              periodSeconds: 10
              initialDelaySeconds: 10
              failureThreshold: 6
            readinessProbe:
              exec:
                command:
                - /bin/calico-node
                - -felix-ready
                - -bird-ready
              periodSeconds: 10
            volumeMounts:
